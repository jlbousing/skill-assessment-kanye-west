{
    "compilerOptions": {
        "target": "ES2016",
        "module": "CommonJS",
        "paths": {
            "@/*": "./resources/js/*",
        },
        "isolatedModules": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noImplicitAny": false
    }
}
